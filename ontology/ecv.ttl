###  http://purl.org/eiffo/ecv#EarthRadiationBudgetECV1
:EarthRadiationBudgetECV1 rdf:type owl:NamedIndividual ,
                                   :ECV ;
                          :hasDataSource :DS_005 ,
                                         :DS_006 ;
                          :hasDomain :UpperAirDomain1 ;
                          :hasECVProduct :ECVProduct_009 ,
                                         :ECVProduct_010 ,
                                         :ECVProduct_011 ,
                                         :ECVProduct_012 ;
                          :hasECVSteward :ECVSteward_04 ;
                          :hasScientificArea :ScientificArea_03 ;
                          :ecvDescription "The Earth Radiation Budget (at the top of the atmosphere) describes the overall balance between the incoming energy from the sun and the outgoing thermal (longwave) and reflected (shortwave) energy from the earth. It can only be measured from space. The radiation balance at the top of the atmosphere is the basic radiative forcing of the climate system. Measuring its variability in space and time over the globe provides insight into the overall response of the system to this forcing." ;
                          :ecvFactsheetLink "https://ane4bf-datap1.s3.eu-west-1.amazonaws.com/wmod8_climatedata/s3fs-public/earth_radiation_ecv_factsheet_201905.pdf?zopx4C.dGHzIKbTkcIOVWWe9ePDAm.8P" ;
                          :ecvIconLink "https://ane4bf-datat.s3.eu-west-1.amazonaws.com/wmod8_climatedata/s3fs-public/ico-uaa-earth-radiation-budget_hover.png?hxQxe_rDseDjf2CZ3y45QZKgEjXvcMR_=" ;
                          :ecvName "Earth Radiation Budget" ;
                          rdfs:label "Earth Radiation Budget" .


