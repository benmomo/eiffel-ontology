###  http://purl.org/eiffo/ecv#AboveGroundBiomassECV1
:AboveGroundBiomassECV1 rdf:type owl:NamedIndividual ,
                                 :ECV ;
                        :hasDataSource :DS_081 ,
                                       :DS_082 ,
                                       :DS_083 ,
                                       :DS_084 ;
                        :hasDomain :BiosphereDomain1 ;
                        :hasECVProduct :ECVProduct_073 ;
                        :hasECVSteward :ECVSteward_21 ,
                                       :ECVSteward_22 ;
                        :hasScientificArea :ScientificArea_06 ;
                        :ecvDescription "Land cover is the observed (bio)-physical cover on the Earth’s surface. It influences climate by modifying water and energy exchanges with the atmosphere and by changing greenhouse gas and aerosol sources and sinks. Land-cover conditions are inherently dynamic (i.e. seasonality) and distributions are linked to regional climatic conditions, so changes in cover can be due to climate change on a regional scale as well as directly due to human activities" ;
                        :ecvFactsheetLink "https://ane4bf-datap1.s3.eu-west-1.amazonaws.com/wmod8_climatedata/s3fs-public/biomass_ecv_factsheet_201905.pdf?UIj2pnLviZJQEYAmaNuHOB3_fWsMjQoS" ;
                        :ecvIconLink "https://ane4bf-datat.s3.eu-west-1.amazonaws.com/wmod8_climatedata/s3fs-public/ico-bio-above-ground-biomass_hover.png?9_9apKhjAgL8jiCQ0E29_w1uo_u7tE1V=" ;
                        :ecvName "Above-ground Biomass" ;
                        rdfs:label "Above-ground Biomass" .


###  http://purl.org/eiffo/ecv#AerosolECV1
:AerosolECV1 rdf:type owl:NamedIndividual ,
                      :ECV ;
             :hasDataSource :DS_005 ,
                            :DS_006 ,
                            :DS_027 ,
                            :DS_035 ,
                            :DS_038 ,
                            :DS_039 ,
                            :DS_040 ,
                            :DS_041 ;
             :hasDomain :AtmosphericCompositionDomain1 ;
             :hasECVProduct :ECVProduct_022 ,
                            :ECVProduct_023 ,
                            :ECVProduct_024 ,
                            :ECVProduct_025 ;
             :hasECVSteward :ECVSteward_04 ,
                            :ECVSteward_10 ;
             :hasScientificArea :ScientificArea_02 ;
             :ecvDescription "Atmospheric aerosols are minor constituents of the atmosphere by mass, but a critical component in terms of impacts on the climate and especially climate changes. Aerosols influence the global radiation balance by directly scattering solar radiation and indirectly through influencing cloud reflectivity, cloud cover and cloud lifetime" ;
             :ecvFactsheetLink "https://ane4bf-datap1.s3.eu-west-1.amazonaws.com/wmod8_gcos/s3fs-public/aerosols_ecv_factsheet_20190130_0.pdf?32jYM14CKwOQ4LSW6Tbriu_LUYlizEcE" ;
             :ecvIconLink "https://ane4bf-datat.s3.eu-west-1.amazonaws.com/wmod8_climatedata/s3fs-public/ico-ac-aerosols_hover.png?19GOEoYMrJnVc1_L04S4QwfyMm9UV6_f=" ;
             :ecvName "Aerosol Properties" ;
             rdfs:label "Aerosol Properties" .


###  http://purl.org/eiffo/ecv#AlbedoECV1
:AlbedoECV1 rdf:type owl:NamedIndividual ,
                     :ECV ;
            :hasDataSource :DS_006 ,
                           :DS_085 ,
                           :DS_086 ;
            :hasDomain :BiosphereDomain1 ;
            :hasECVProduct :ECVProduct_074 ,
                           :ECVProduct_075 ,
                           :ECVProduct_076 ,
                           :ECVProduct_077 ;
            :hasECVSteward :ECVSteward_23 ;
            :hasScientificArea :ScientificArea_03 ;
            :ecvDescription "The land surface albedo is the ratio of the radiant flux reflected from Earth’s surface to the incident flux. It is a key forcing parameter controlling the partitioning of radiative energy between the atmospheric and surface. In the case of vegetation, a reference surface is typically defined at or near the top of the canopy and must be specified explicitly. Surface albedo depends on natural variations, highly variable in space and time as a result of terrestrial properties changes, and with illumination conditions and human activities and is a sensitive indicator of environmental vulnerability" ;
            :ecvFactsheetLink "https://ane4bf-datap1.s3.eu-west-1.amazonaws.com/wmod8_climatedata/s3fs-public/albedo_ecv_factsheet_201905.pdf?_UGkfFiOyAusFLwiLAP54ty4plAVXXf7" ;
            :ecvIconLink "https://ane4bf-datat.s3.eu-west-1.amazonaws.com/wmod8_climatedata/s3fs-public/ico-bio-albedo_hover.png?MhkdFeueNURaJhR7YMQhLsIwgFk9AYjm=" ;
            :ecvName "Albedo" ;
            rdfs:label "Albedo" .


###  http://purl.org/eiffo/ecv#AnthropogenicGHGEmissionsECV1
:AnthropogenicGHGEmissionsECV1 rdf:type owl:NamedIndividual ,
                                        :ECV ;
                               :hasDataSource :DS_084 ,
                                              :DS_107 ,
                                              :DS_108 ,
                                              :DS_109 ,
                                              :DS_110 ,
                                              :DS_111 ;
                               :hasDomain :AnthroposphereDomain1 ;
                               :hasECVProduct :ECVProduct_098 ,
                                              :ECVProduct_099 ,
                                              :ECVProduct_100 ,
                                              :ECVProduct_101 ,
                                              :ECVProduct_102 ;
                               :hasECVSteward :ECVSteward_30 ,
                                              :ECVSteward_31 ;
                               :hasScientificArea :ScientificArea_06 ;
                               :ecvDescription "Global anthropogenic emissions of Greenhouse gases (CO2, CH4, N2O and F-gases) continue to be emitted at an annual rate that is not yet  significantly decreasing. The global warming potential of each of the greenhouse gases and their long residence time in the atmosphere are causing increased surface temperature and climate change. The scientific community illustrated with inverse models and data assimilation how consistent the reported inventories and the atmospheric observations are, which is taken up also in few national inventory reports (e.g. UK, Switzerland, Australia)" ;
                               :ecvFactsheetLink "https://ane4bf-datap1.s3.eu-west-1.amazonaws.com/wmod8_climatedata/s3fs-public/anthropogenic_ghg_ecv_factsheet_201905.pdf?T1.d30Ea8EwukWhqM0g4VqZJkkk0tDOP" ;
                               :ecvIconLink "https://ane4bf-datat.s3.eu-west-1.amazonaws.com/wmod8_climatedata/s3fs-public/ico-ant-ghg-fluxes_hover.png?rTpiuZyu8uQDWXGDji42qDff6CNC3tHH=" ;
                               :ecvName "Anthropogenic Greenhouse Gas Emissions" ;
                               rdfs:label "Anthropogenic Greenhouse Gas Emissions" .


###  http://purl.org/eiffo/ecv#AnthropogenicWaterUseECV1
:AnthropogenicWaterUseECV1 rdf:type owl:NamedIndividual ,
                                    :ECV ;
                           :hasDataSource :DS_112 ,
                                          :DS_113 ;
                           :hasDomain :AnthroposphereDomain1 ;
                           :hasECVProduct :ECVProduct_103 ;
                           :hasECVSteward :ECVSteward_32 ;
                           :hasScientificArea :ScientificArea_01 ;
                           :ecvDescription "Data on water extractions by sector and available renewable freshwater provide key information on the availability of freshwater and the amount of water stress in a country. Climate Change is projected to reduce  renewable surfacewater and groundwater resources significantly in most dry subtropical regions. In contrast, water resources are projected to increase at high latitudes. Climate change is also projected to reduce raw water quality, posing risks to drinking water quality, even with conventional treatment. The availability of freshwater plays a crucial role in food production and food security." ;
                           :ecvFactsheetLink "https://ane4bf-datap1.s3.eu-west-1.amazonaws.com/wmod8_climatedata/s3fs-public/anthropogenic_water_ecv_factsheet_201905.pdf?oQFYGQXzcRd7e0OKlSXO.G3gX2BlKFUd" ;
                           :ecvIconLink "https://gcos.wmo.int/en/essential-climate-variables/water-use/" ;
                           :ecvName "Anthropogenic Water Use" ;
                           rdfs:label "Anthropogenic Water Use" .


